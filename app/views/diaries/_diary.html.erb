<li id="diary-<%= diary.id %>">
  <span class="title"><%= link_to diary.title, diary %></span>
    <%= image_tag('/key.jpg') if diary.shared_with == SHARED_WITH_FRIENDS_ONLY %>
  <dev class="timestamp">
    <%= link_to diary.user.name, diary.user %> Posted
    <%= time_ago_in_words(diary.created_at) %> ago.
  </dev>
</li>
